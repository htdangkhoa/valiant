  "scripts": {
    "start": "electron .",
    "dev:main": "env-cmd -f .env webpack",
    "dev:render": "env-cmd -f .env webpack serve --config webpack.config.renderer.js",
    "dev": "npm-run-all --parallel dev:render dev:main",
    "build:main": "env-cmd -f .env webpack",
    "build:renderer": "env-cmd -f .env webpack --config webpack.config.renderer.js",
    "prebuild": "rimraf dist build && npm-run-all --sequential build:main build:renderer",
    "build": "env-cmd -f .env electron-packager . --out=build --overwrite"
  },
